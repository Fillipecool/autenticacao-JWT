{
    "name": "autenticacao-jwt/api-rest",
    "description": "Uma API REST em PHP usando Slim Framework",
    "require": {
        "php": ">=7.4",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "monolog/monolog": "^2.0",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
